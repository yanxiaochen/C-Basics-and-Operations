#include<stdio.h>
int main()
{
int a[20]={1,1},x;    //二维数组的定义,行、列的第一个数据都设为1
    for(x=2;x<=19;x++)
         a[x]=a[x-1]+a[x-2];
     for(x=0;x<=19;x++)
 {
	if(x%4==0)    //用来实现每行输出几个数据
	printf("\n");
        printf("%8d",a[x]);
 }
      printf("\n");
	return 0;
 } 
 
 
 //这道题为经典的二维数组题目，我是输出20个数，并每行只输出4个数据

